{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false
  },
  "extends": ["@nuxtjs", "plugin:nuxt/recommended", "prettier"],
  "plugins": ["@typescript-eslint"],
  "ignorePatterns": ["store/index.ts"],
  "rules": {
    "vue/require-prop-types": "off"
  }
}
